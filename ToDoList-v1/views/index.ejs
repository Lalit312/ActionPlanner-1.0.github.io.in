

    <%- include("header") -%>

    <div class="box" id="heading">
        <h1 style="color:black"><%= listTitle %> </h1>

    </div>

    
    <div class="box">
            <%  for(var i=0;i<newlistItems.length;i++) { %>

                <div class="item">
                    <input type="checkbox">
                     <p> <%= newlistItems[i] %> </p>
                </div>
                  
            <% } %>

            <form class="item" action="/" method="POST" autocomplete="off">
                <input type="text" name="addItem" placeholder="Add Item">
                <button type="submit" name="list" value="<%= listTitle %>">+</button>
            </form>
    </div>
    
    <%- include("footer") -%>